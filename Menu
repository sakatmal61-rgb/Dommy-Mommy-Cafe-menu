const modal=document.getElementById('passwordModal'),
      menu=document.getElementById('secretMenu'),
      input=document.getElementById('passwordInput'),
      error=document.getElementById('passwordError');

function toggleModal(show){
  modal.style.display = show ? 'flex' : 'none';
  input.value = '';
  error.style.display = 'none';
}

function checkPassword(){
  if(input.value==='mommyy'){
    toggleModal(false);
    menu.style.display='block';
    document.getElementById('btnText').textContent='Hide Secret Menu';
  } else {
    error.style.display='block';
    input.value='';
  }
}

modal.addEventListener('click', e => { if(e.target===modal) toggleModal(false) });
input.addEventListener('keypress', e => { if(e.key==='Enter') checkPassword() });
